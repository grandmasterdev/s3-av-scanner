{
  "dev": {
    "incomingBucketArnList": [
      "arn:aws:s3:::somebucket-1",
      "arn:aws:s3:::somebucket-2"
    ],
    "defaultIncomingBucket": true,
    "defaultInfectedBucket": false,
    "scanningAgentLambda": {
      "memorySize": 128
    },
    "scanningAgentAv": {
      "enableWaf": true,
      "incomingBucketLifeCycle": 1,
      "infectedBucketLifeCycle": 1,
      "scannedBucketLifeCycle": 1
    }
  },
  "prod": {
    "incomingBucketArnList": [
      "arn:aws:s3:::somebucket-1",
      "arn:aws:s3:::somebucket-2",
      "arn:aws:s3:::somebucket-3"
    ],
    "defaultIncomingBucket": true,
    "defaultInfectedBucket": false,
    "scanningAgentLambda": {
      "memorySize": 128
    },
    "scanningAgentAv": {
      "enableWaf": true,
      "incomingBucketLifeCycle": 1,
      "infectedBucketLifeCycle": 1,
      "scannedBucketLifeCycle": 1
    }
  }
}
